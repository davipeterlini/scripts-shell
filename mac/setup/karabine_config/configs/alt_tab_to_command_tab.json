{
    "title": "Windows-style Alt+Tab",
    "rules": [
        {
            "description": "Enable Windows-style Alt+Tab behavior",
            "manipulators": [
                {
                    "description": "Alt+Tab -> Command+Tab (forward)",
                    "from": {
                        "key_code": "tab",
                        "modifiers": {
                            "mandatory": ["left_option"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "tab",
                            "modifiers": ["left_command"]
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}